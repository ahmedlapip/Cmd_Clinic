<?xml version="1.0" encoding="UTF-8"?>
<module type="JAVA_MODULE" version="4">
  <component name="NewModuleRootManager" inherit-compiler-output="true">
    <exclude-output />

    <!-- Content Root -->
    <content url="file://$MODULE_DIR$">
      <sourceFolder url="file://$MODULE_DIR$/src" isTestSource="false" />
      <!-- If you have test sources, you can uncomment and modify the following: -->
      <!-- <sourceFolder url="file://$MODULE_DIR$/test" isTestSource="true" /> -->
    </content>

    <!-- Module Dependencies -->
    <orderEntry type="inheritedJdk" />

    <!-- Adding source folder for regular source code -->
    <orderEntry type="sourceFolder" forTests="false" />

    <!-- Library Dependency (For Java EE 6) -->
    <orderEntry type="library" name="Java EE 6-Java EE 6" level="project" />

    <!-- Add any other library dependencies like JavaMail -->
    <!-- Example for JavaMail, if it's required -->
    <orderEntry type="library" name="javax.mail" level="project" />
  </component>
</module>
